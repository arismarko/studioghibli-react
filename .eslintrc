{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/require-extension": "off",
    "linebreak-style": "off",
    "no-param-reassign": 0,
     "import/prefer-default-export": "off",
     "arrow-body-style": "off"
  },
  "env": {
    "browser": true
  },
   "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
}


